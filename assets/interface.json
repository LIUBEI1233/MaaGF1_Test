    "81N_是否开启无限循环": {
      "cases": [
        {
          "name": "开启无限循环",
          "pipeline_override": {
            "81N重置_重新作战": {
              "next": [
                "81N部署_点击2队new"
              ]
            }
          }
        },
        {
          "name": "关闭无限循环"
        }
      ]
    },
    "81N_装备溢出处理": {
      "cases": [
        {
          "name": "专属装备强化",
          "pipeline_override": {
            "81N_选择装备处理方式": {
              "interrupt": [
                "public装备强化_装备强化",
                "public装备强化_导航栏战斗",
                "public_进入导航栏"
              ]
            }
          }
        },
        {
          "name": "拆解（锁上需保留装备）",
          "pipeline_override": {
            "81N_选择装备处理方式": {
              "interrupt": [
                "public拆装备_装备回收",
                "public装备强化_导航栏战斗",
                "public_进入导航栏"
              ]
            }
          }
        },
        {
          "name": "结束任务",
          "pipeline_override": {
            "81N_选择装备处理方式": {
              "timeout": 1000,
              "interrupt": []
            }
          }
        }
      ]
    },
